@page "/commanddetail/{Id}"

<h1>Command Detail - @Command.HowTo</h1>

<div class="row">
    <div class="col-md-12">
        <div class="form-group row">
            <label class="col-md-4">Id</label>
            <div class="col-md-8">
                <label class="form-control">@Command.Id</label>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-4">HowTo</label>
            <div class="col-md-8">
                <label class="form-control">@Command.HowTo</label>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-4">Line</label>
            <div class="col-md-8">
                <label class="form-control">@Command.Line</label>
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-4">Platform</label>
            <div class="col-md-8">
                <label class="form-control">@Command.Platform</label>
            </div>
        </div>
    </div>
</div>